import java.util.Scanner;
public class Program {
	public static void main(String[] args) {
     Double[] salario = new Double[5];
     Double promedio=0.0;
     Scanner lector = new Scanner(System.in);
     
     for(int cont=0;cont<5;cont++){
     System.out.println("digite salario");
        salario[cont]= lector.nextDouble(); 
     }
     promedio=promediosalario(salario);
     System.out.println("El promedio de salario es:"+promedio);
	}
    public static Double promediosalario(Double[] salario){
    Double suma=0.0;
        Double prom=0.0;
        
        for(int cont=0;cont<5;cont++){
        suma=suma+salario[cont];
            
        }
        prom=suma/5;
        return prom;
    }
}
